{
    "index_patterns": [
        "ranking-news-hour-naver-*"
    ],
    "template": {
        "settings": {
            "number_of_shards": null
        },
        "mappings": {
            "_source": {
                "enabled": true
            },
            "properties": {
                "news_nm": {
                    "type": "text",
                    "fielddata": true
                },
                "url": {
                    "type": "keyword"
                },
                "create_time": {
                    "type": "date"
                },
                "modified_time": {
                    "type": "date"
                },
                "view": {
                    "type": "integer"
                },
                "reaction": {
                    "type": "integer"
                },
                "comment": {
                    "type": "integer"
                },
                "platform": {
                    "type": "keyword"
                },
                "reg_date": {
                    "type": "date"
                },
                "publisher": {
                    "type": "keyword"
                }
            }
        },
        "aliases": {

        }
    }
}